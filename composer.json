{
  "name": "dravencms/article",
  "type": "dravencms-package",
  "description": "Article module for dravencms",
  "keywords": [
    "dravencms",
    "article"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {    
    "dravencms/admin": ">=2.0",
    "dravencms/tag": ">=2.0",
    "dravencms/file": ">=2.0",
    "dravencms/paginator": ">=1.0"
  },
  "suggest": {
    "dravencms/structure": "Allows article components to be used in cms"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": "dravencms.config.neon"
    }
  }
}
